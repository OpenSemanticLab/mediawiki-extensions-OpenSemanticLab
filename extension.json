{
   "name": "LabWiki",
	"version": "0.0.1",
	"author": [
		"Simon Stier"
	],
	"url": "https://github.com/simontaurus/LabWiki",
	"descriptionmsg": "labwiki-desc",
	"namemsg": "labwiki-name",
	"license-name": "GPL v3",
	"type": "semantic",
	"requires": {
		"MediaWiki": ">= 1.31"
	},
	"MessagesDirs": {
		"LabWiki": [
			"i18n"
		]
	},
	"manifest_version": 1,
	"ResourceModules": {
		"ext.LabWiki.styles": {
			"styles": [
				"LabWiki.css"
			],
			"position": "bottom"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "LabWiki/resources"
	},
	"namespaces": [
		{
			"id": 7010,
			"constant": "NS_MATERIAL",
			"name": "Material"
		},
		{
			"id": 7011,
			"constant": "NS_MATERIAL_TALK",
			"name": "Material_Talk"
		},
		{
			"id": 7020,
			"constant": "NS_DEVICE",
			"name": "Device"
		},
		{
			"id": 7021,
			"constant": "NS_DEVICE_TALK",
			"name": "Device_Talk"
		},
		{
			"id": 7030,
			"constant": "NS_ROOM",
			"name": "Room"
		},
		{
			"id": 7031,
			"constant": "NS_ROOM_TALK",
			"name": "Room_Talk"
		},
		{
			"id": 7100,
			"constant": "NS_LABNOTE",
			"name": "LabNote"
		},
		{
			"id": 7101,
			"constant": "NS_LABNOTE_TALK",
			"name": "LabNote_Talk"
		},
		{
			"id": 7110,
			"constant": "NS_LABPROCESS",
			"name": "LabProcess"
		},
		{
			"id": 7111,
			"constant": "NS_LABPROCESS_TALK",
			"name": "LabProcess_Talk"
		},
		{
			"id": 7120,
			"constant": "NS_LABOBJECT",
			"name": "LabObject"
		},
		{
			"id": 7121,
			"constant": "NS_LABOBJECT_TALK",
			"name": "LabObject_Talk"
		}
	],
	"attributes": {
		"VisualEditor": {
			"AvailableNamespaces": {
				"7100": true,
				"7200": true
			}
		}
	},
	"AutoloadClasses": {
		"LabWiki": "includes/LabWiki.php"
	},
	"Hooks": {
		"PageImporterRegisterPageLists": "LabWiki::onPageImporterRegisterPageLists",
		"BeforePageDisplay": "LabWiki::onBeforePageDisplay",
		"SetupAfterCache": "LabWiki::onSetupAfterCache"
	}
}
